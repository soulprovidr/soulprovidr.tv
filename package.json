  "author": "Shola Anozie",
  "scripts": {
    "app:build": "yarn && webpack --env.NODE_ENV=prod --mode production",
    "app:deploy": "yarn build && node bin/deploy",
    "app:lint": "eslint -c .eslintrc --ext .js,.jsx src",
    "app:start": "webpack-dev-server --env.NODE_ENV=dev --hot --mode development",
    "lambda:deploy": "node-lambda deploy -G src/lambda/save-video -f src/lambda/save-video/deploy.env",
    "lambda:install": "cd src/lambda/save-video && yarn",
    "sync-playlists": "node bin/sync"
  },
